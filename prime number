#include <stdio.h>
int check_prime(int);
int main() {
    int n;
    printf("Enter number\n");
    scanf("%d",&n);
    if(check_prime(n))
    {
        printf("%d is prime",n);
        
    }
    else{
        printf("%d is not prime",n);
    }
    return 0;
}
int check_prime(int n){
    int is_prime=1;
    if(n==1)
    {
        return 0;
    }
    for(int i=2;i<n;i++)
    {
        if(n%i==0)
        {
            is_prime=0;
            break;
        }
    }
    return is_prime;
}
