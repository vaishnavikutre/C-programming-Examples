#include<stdio.h>
int main(){
    int n,reminder,sum=0;
    printf("Enter number\n");
    scanf("%d",&n);
    
    while(n/10 !=0)
    {
        sum=0;
         while(n!=0){
        reminder=n%10;
        sum=reminder+sum;
        n=n/10;
    }
        n=sum;
    }
   
    printf("%d",sum);
    return 0;
}
